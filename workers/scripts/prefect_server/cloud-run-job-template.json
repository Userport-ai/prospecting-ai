{
  "variables": {
    "type": "object",
    "properties": {
      "env": {
        "type": "object",
        "properties": {
          "PREFECT_API_URL": {
            "type": "string",
            "default": "https://prefect-server-12345.us-east1.run.app/api"
          },
          "PREFECT_API_AUTH_STRING": {
            "type": "string",
            "default": "userport:cFyQ4xxU2fW_CYAa43qip8E"
          }
        },
        "description": "Environment variables to set in the container.",
        "additionalProperties": false
      },
      "name": {
        "type": "string",
        "default": "my-cloud-run-job",
        "description": "Name of the Cloud Run Job"
      },
      "labels": {
        "type": "object",
        "description": "Labels to apply to the Cloud Run Job",
        "additionalProperties": {
          "type": "string"
        }
      }
    }
  },
  "job_configuration": {
    "region": "us-east1",
    "job_body": {
      "kind": "Job",
      "spec": {
        "template": {
          "spec": {
            "template": {
              "spec": {
                "containers": [
                  {
                    "env": "{{env}}",
                    "image": "prefecthq/prefect:3-latest"
                  }
                ]
              }
            }
          }
        }
      },
      "metadata": {
        "name": "{{name}}",
        "labels": "{{labels}}",
        "annotations": {
          "run.googleapis.com/region": "us-east1",
          "run.googleapis.com/launch-stage": "BETA"
        }
      },
      "apiVersion": "run.googleapis.com/v1"
    }
  }
}